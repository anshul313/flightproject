{
  "name": "levo-api",
  "description": "Custom & Chat APIs for Levo",
  "author": "<Tanmai Gopal> <<tanmaig@hasura.io>>",
  "license": "MIT",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/hasura-support/levo-chat"
  },
  "homepage": "",
  "keywords": [
    "hasura",
    "levo",
    "nodemon",
    "express",
    "starter",
    "boilerplate",
    "babel",
    "eslint"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start-prod": "better-npm-run start-prod",
    "lint": "eslint -c .eslintrc src",
    "start-dev": "better-npm-run start-dev"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "nodemon ./bin/server.js --exec '`npm bin`/eslint -c .eslintrc src && node'",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "body-parser": "^1.15.2",
    "express": "^4.13.3",
    "fcm-push": "^1.1.2",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "^1.7.0",
    "node-fetch": "^1.6.0",
    "nodemailer": "^2.7.0",
    "nodemon": "^1.10.0",
    "pretty-error": "^1.2.0",
    "socket.io": "^1.4.8"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-plugin-typecheck": "^3.6.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "better-npm-run": "^0.0.8",
    "eslint": "^3.1.0",
    "eslint-config-airbnb-base": "^5.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.0.0"
  },
  "engines": {
    "node": "4.4.7"
  }
}
